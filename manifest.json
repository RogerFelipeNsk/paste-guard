{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.1.0",
  "default_locale": "en",
  "permissions": [],
  "host_permissions": ["<all_urls>"],
  "options_page": "landing.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
